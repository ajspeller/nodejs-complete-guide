<%- include('../includes/head.ejs') %> 

</head>
<body>
  
  <%- include('../includes/navigation.ejs') %>
  <main>

    <% if (orders.length < 1) { %>
      <h1>No Orders</h1>
    <% } else { %>
      <ul>
        <% orders.forEach(o => { %>
          <li>
            <h1># <%= o.id %> </h1>
            <ul>
              <% o.products.forEach(p => { %>
                <li><%= p.title %> (<%= p.orderItem.quantity %>) </li>
              <% }) %>
            </ul>
          </li>
        <% }) %>
      </ul>
    <% } %>

  </main>
  <%- include('../includes/end.ejs') %>